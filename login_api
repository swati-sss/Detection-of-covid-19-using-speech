    public func setEnvironment(_ environment: PluginAPIs.CompassEnvironment) {
        let sdkEnv: compass_sdk_ios.CompassEnvironment
        switch environment {
        case .staging:
            sdkEnv = .staging
        case .production:
            sdkEnv = .production
        case .perf:
            sdkEnv = .perf
        case .preProd:
            sdkEnv = .preProd
        case .stageGKE:
            sdkEnv = .stageGKE
        }
        compass?.setEnvironment(sdkEnv)
    }
