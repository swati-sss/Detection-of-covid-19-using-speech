 #if DEBUG
     if forceNavigationButtonVisible {
         navigationButton.isHidden = false
         let iconColor = isNavigationButtonClicked ? MapButtonColor.black : .white
         let backgroundColor = isNavigationButtonClicked ? .white : MapButtonColor.blue
         let image = isNavigationButtonClicked
             ? Asset.Image.routePreviewStop.image
             : Asset.Image.routePreviewStart.image
         applyIconButtonStyle(
             navigationButton,
             image: image,
             iconColor: iconColor,
             backgroundColor: backgroundColor
         )
         navigationButton.accessibilityIdentifier =
             isNavigationButtonClicked ? "routePreviewStop" : "routePreviewStart"
         return
     }
     #endif
