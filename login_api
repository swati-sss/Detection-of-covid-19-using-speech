override func viewWillAppear(_ animated: Bool) {
    super.viewWillAppear(animated)
    
    // If returning from another screen and compass exists
    if compass != nil && compass?.currentStore == currentStore {
        // Simply re-trigger the position status and map display
        compass?.resetPositionStatusEvent()
        compass?.displayMap(workflow: Workflow(id: "sample_app_id", type: "sample_app_flow", value: "sa_val"))
    }
}
